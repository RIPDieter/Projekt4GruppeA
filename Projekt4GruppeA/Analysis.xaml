<Window x:Class="Projekt4GruppeA.Analysis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt4GruppeA"
        xmlns:sparrow="http://sparrowtoolkit.codeplex.com/wpf"
        mc:Ignorable="d"
        Title="Analysis" Height="400" Width="400"
        ResizeMode="NoResize"
        Loaded="Window_Loaded"
        >

    <Grid x:Name="gr_statisticsGrid">


        <Label Content="Gesamt:" Margin="31,13,203,329" Height="30" Width="160"/>
        <TextBlock x:Name="tb_totalCarCountOnMap" Width="60" Text="XXX" VerticalAlignment="Center" Height="30" Margin="213,13,121,329"/>

        <Label Content="Von Links nach Rechts" Height="30" Margin="17,48,217,294" Width="160"/>
        <TextBlock x:Name="tb_leftToRightCars" Width="60" Text="XXX" Height="30" VerticalAlignment="Center" Margin="213,53,121,289"/>

        <Label Content="Von Rechts nach Links" Height="30" Margin="17,83,217,259" Width="160"/>
        <TextBlock x:Name="tb_rightToLeftCars" Width="60" Text="XXX" Height="30" VerticalAlignment="Center" Margin="213,83,121,259"/>

        <sparrow:SparrowChart Margin="10,162,10,10" x:Name="ch_sparrowChart">
            <sparrow:SparrowChart.XAxis>
                <sparrow:LinearXAxis/>
            </sparrow:SparrowChart.XAxis>
            <sparrow:SparrowChart.YAxis>
                <sparrow:LinearYAxis/>
            </sparrow:SparrowChart.YAxis>
            <sparrow:LineSeries>
                <sparrow:LineSeries.Points>
                    <sparrow:DoublePoint Data="0" Value="1" x:Name="ch_p1"/>
                    <sparrow:DoublePoint Data="1" Value="5"/>
                    <sparrow:DoublePoint Data="2" Value="3"/>
                    <sparrow:DoublePoint Data="3" Value="4"/>
                </sparrow:LineSeries.Points>
            </sparrow:LineSeries>
        </sparrow:SparrowChart>
        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Label x:Name="label1" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="67,118,0,0" Width="80"/>

    </Grid>

</Window>