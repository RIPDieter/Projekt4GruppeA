<Window x:Class="Projekt4GruppeA.Analysis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt4GruppeA"
        xmlns:sparrow="http://sparrowtoolkit.codeplex.com/wpf"
        mc:Ignorable="d"
        Title="Analysis" Height="400" Width="400"
        ResizeMode="NoResize"
        Loaded="Window_Loaded"
        >

    <Grid x:Name="gr_statisticsGrid">


        <Label Content="Gesamt:" Margin="10,8,224,334" Height="30" Width="160"/>
        <TextBlock x:Name="tb_totalCarCountOnMap" Width="60" Text="XXX" VerticalAlignment="Center" Height="30" Margin="200,13,134,329"/>

        <Label Content="Von Links nach Rechts" Height="30" Margin="10,43,224,299" Width="160"/>
        <TextBlock x:Name="tb_leftToRightCars" Width="60" Text="XXX" Height="30" VerticalAlignment="Center" Margin="200,48,134,294"/>

        <Label Content="Von Rechts nach Links" Height="30" Margin="10,73,224,269" Width="160"/>
        <TextBlock x:Name="tb_rightToLeftCars" Width="60" Text="XXX" Height="30" VerticalAlignment="Center" Margin="200,78,134,264"/>

        <sparrow:SparrowChart Margin="10,162,10,10" x:Name="ch_sparrowChart">
            <sparrow:SparrowChart.XAxis>
                <sparrow:LinearXAxis/>
            </sparrow:SparrowChart.XAxis>
            <sparrow:SparrowChart.YAxis>
                <sparrow:LinearYAxis/>
            </sparrow:SparrowChart.YAxis>
            <sparrow:LineSeries>
                <sparrow:LineSeries.Points>
                    <sparrow:DoublePoint Data="0" Value="1" x:Name="ch_p1"/>
                    <sparrow:DoublePoint Data="1" Value="5"/>
                    <sparrow:DoublePoint Data="2" Value="3"/>
                    <sparrow:DoublePoint Data="3" Value="4"/>
                </sparrow:LineSeries.Points>
            </sparrow:LineSeries>
        </sparrow:SparrowChart>
        <Label x:Name="tmrCount" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,103,0,0" Width="80" Content="Timer"/>
        <TextBlock x:Name="txtTimer" HorizontalAlignment="Left" Margin="200,108,0,0" TextWrapping="Wrap" Text="XXX" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="265,48,0,0" Height="55" Width="96" FontSize="18.667"/>

    </Grid>

</Window>