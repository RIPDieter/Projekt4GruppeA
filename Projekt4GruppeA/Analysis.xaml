<Window x:Class="Projekt4GruppeA.Analysis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt4GruppeA"
        xmlns:sparrow="http://sparrowtoolkit.codeplex.com/wpf"
        mc:Ignorable="d"
        Title="Analysis" Height="500" Width="500"
        ResizeMode="NoResize"
        Loaded="Window_Loaded"
        >

    <Grid x:Name="gr_statisticsGrid">


        <Label Content="Gesamt:" Margin="10,96,324,346" Height="30" Width="160"/>
        <TextBlock x:Name="tb_totalCarCountOnMap" Width="60" Text="XXX" VerticalAlignment="Center" Height="30" Margin="160,46,274,396"/>

        <Label Content="Von Links nach Rechts" Height="30" Margin="10,36,324,406" Width="160"/>
        <TextBlock x:Name="tb_leftToRightCars" Width="60" Text="XXX" Height="30" VerticalAlignment="Center" Margin="160,76,274,366"/>

        <Label Content="Von Rechts nach Links" Height="30" Margin="10,66,324,376" Width="160"/>
        <TextBlock x:Name="tb_rightToLeftCars" Width="60" Text="XXX" Height="30" VerticalAlignment="Center" Margin="110,106,324,336"/>

        <sparrow:SparrowChart Margin="10,162,10,10" x:Name="ch_sparrowChart" RenderTransformOrigin="0.495,0.482">
            <sparrow:SparrowChart.XAxis>
                <sparrow:LinearXAxis/>
            </sparrow:SparrowChart.XAxis>
            <sparrow:SparrowChart.YAxis>
                <sparrow:LinearYAxis/>
            </sparrow:SparrowChart.YAxis>
            <sparrow:LineSeries>
                <sparrow:LineSeries.Points>
                    <sparrow:DoublePoint Data="0" Value="1" x:Name="ch_p1"/>
                    <sparrow:DoublePoint Data="1" Value="5"/>
                    <sparrow:DoublePoint Data="2" Value="3"/>
                    <sparrow:DoublePoint Data="3" Value="4"/>
                </sparrow:LineSeries.Points>
            </sparrow:LineSeries>
        </sparrow:SparrowChart>
        <Label x:Name="tmrCount" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Width="80" Content="Timer"/>
        <TextBlock x:Name="txtTimer" HorizontalAlignment="Left" Margin="120,15,0,0" TextWrapping="Wrap" Text="XXX" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="225,10,0,0" Height="43" Width="259" FontSize="18.667"/>

    </Grid>

</Window>